geom_bar(stat = "identity", position = "dodge") +
scale_fill_manual(values = c("lake" = "blue", "creek" = "green")) +
labs(x = "Month", y = "Total Count") +
ggtitle("Species Count in Lake and Creek") +
theme_minimal() +
theme(legend.title = element_blank())
